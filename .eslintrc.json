{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    // "plugin:react/recommended"
    // "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": "off",
    // client와 상의 후 조율할 예정
    "no-use-before-define": "off",
    "react/style-prop-object": "off"
  }
}
